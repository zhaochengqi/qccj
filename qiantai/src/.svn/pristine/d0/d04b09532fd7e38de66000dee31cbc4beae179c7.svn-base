@media (min-width: 768px) {
  .container {
    width: 750px;
  }
}
@media (min-width: 992px) {
  .container {
    width: 970px;
  }
}
@media (min-width: 1200px) {
  .container {
    width: 1170px;
  }
  .zhiku-box{
		width: 20%;
	}
}
@media(min-width:1280px) {
	.container{
		width: 1200px;
	}
}

@media(max-width: 992px){
  .el-menu--horizontal>li.el-menu-item, .el-submenu__title{
    padding: 0 4px;
  }
}

@media(max-width: 768px){
  #content-container{
    padding-top: 1px;
  }
  .container.margin-top-50{
    margin-top: 15px!important;
  }
  .container.margin-top-50.padding-top-30{
    padding-top: 10px!important;
  }
  .index-news-banner{
    height: 200px;
    margin-bottom: 20px;
  }
  .index-zixun-img{
    height: 200px;
    margin-bottom: 20px;
  }
  .zixun-date.margin-top-50{
    margin-top: 20px!important;
  }
  .index .el-col-offset-1{
    margin-left: 0!important;
  }
  .index .vjs_video_731-dimensions{
    height: 250px;
  }
  .index-video{
    margin-bottom: 25px;
  }
  .index-yuedu.margin-left-50{
      margin: 0 0 50px!important;
  }
  .yuedu-img{
    height: 175px;
  }
  .header .el-dialog__header{
    padding: 20px 20px 0;
  }
  .header .el-dialog__headerbtn .el-dialog__close{
    right: 20px;
  }
  .header .el-dialog__body button{
    margin: 0!important;
  }
  .header .el-dialog__body button.margin-right-50{
    margin-bottom: 20px!important;
  }
  .el-dialog--tiny{
    width: 80%!important;
  }
  .header .el-form-item__label{
    width: 100%;
    text-align: left;
    float: none;
    margin-left: 20px;
    display: block;
  }
  .header .el-form-item__content{
    margin-left: 20px!important;;
  }
  .header .qiye-duijie .el-checkbox{
    margin-left: 0;
  }
  .daoshi-img{
    height: 115px;
  }
  .ds-more.margin-top-30{
    margin-top: 15px!important;
  }
}