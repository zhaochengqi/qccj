/*返回顶部*/
.scroll-top{

}
.scroll-top.in{
	background-color: #e2534b;
	border-radius: 3px;
	outline: none;
	border: 0;
	font-size: 14px;
	line-height: 20px;
	padding: 7px 12px;
}
.scroll-top.in i{
	-webkit-transform: rotate(90deg);
	   -moz-transform: rotate(90deg);
	    -ms-transform: rotate(90deg);
	     -o-transform: rotate(90deg);
	        transform: rotate(90deg);
	display: inline-block;
	font-weight: normal;
	font-size: 1em;
	margin-left: 36px;
}

